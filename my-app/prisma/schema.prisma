// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Club {
  id   Int    @id @default(autoincrement())
  name  String
  link  String
}

model Badge {
  id   Int    @id @default(autoincrement())
  name  String
  type  String

}
